{
    "name": "liquidstack/core",
    "description": "Core de arranque, routing y helpers para el framework Liquid Stack (repositorio: liquidstack-core). Incluye mÃ³dulos JS/SCSS con animaciones GSAP",
    "type": "composer-plugin",
    "license": "MIT",
    "homepage": "https://github.com/LiquidArtDevelopers/liquidstack-core",
    "require": {
        "php": ">=8.1",
        "composer-plugin-api": "^2.0",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^6.9",
        "symfony/filesystem": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\Core\\": "src/Core/"
        },
        "files": [
            "src/Core/Support/helpers.php"
        ]
    },
    "scripts": {
        "liquidstack-core:sync-resources": [
            "App\\Core\\Composer\\Installer::syncResources"
        ]
    },
    "extra": {
        "class": "App\\Core\\Composer\\Plugin",
        "repository_name": "liquidstack-core"
    },
    "support": {
        "source": "https://github.com/LiquidArtDevelopers/liquidstack-core",
        "issues": "https://github.com/LiquidArtDevelopers/liquidstack-core/issues"
    }
}
